	author = {Huang, Liangwei and Lin, Qingqing and Zhang, Xinyuan and Shuai, Ping},
	year = {2017},
	keywords = {Acceptance space, ambiguity resolution (AR), hypothesis testing, matching search, pulsar-based navigation (PNAV)},
	pages = {137--147},
}

